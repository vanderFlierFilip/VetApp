@{
    @model SettingsViewModel
}

<div class="row-12">
    <div class="col">
        @foreach (var item in @Model.PendingMigrations)
        {
            <div class="alert " role="alert">
                @item
            </div>  
        }

    </div>
    <div class="col">
         @foreach (var item in @Model.AppliedMigrations)
        {
            <div class="alert alert-success" role="alert">
                @item
            </div>  
        }
    </div>
</div>
